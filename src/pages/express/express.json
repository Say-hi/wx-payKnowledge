// Window configuration
{
  // 类型：HexColor；默认值：#000000
  // 导航栏背景颜色，如"#000000"
  "navigationBarBackgroundColor": "#cd9b00",
  // 类型：String；默认值：white
  // 导航栏标题颜色，仅支持 black/white
  // 类型：String；默认值：无
  // 导航栏标题文字内容
  // 类型：HexColor；默认值：#ffffff
  // 窗口的背景色
  "backgroundColor": "#cd9b00",
  // 是否开启下拉刷新，详见页面相关事件处理函数。
  "enablePullDownRefresh": false
}
