<view class="zw"></view>
<view class="operation-wrap">
    <view class="h-a-operation">
        <view catchtap="giveTip" data-index="{{ index }}" class="jwq lh70 f24 main-text-color icon-zhifufangshi">40打赏</view>
        <view catchtap="zan" data-index="{{ index }}" data-id="{{ item.id }}" class="jwq lh70 f24 c999 {{ item.zan ? 'icon-xin1' : 'icon-xin' }}">40喜欢</view>
        <navigator url="../commentList/commentList" class="jwq lh70 f24 c999 icon-pinglun">50评论</navigator>
    </view>
</view>
<my-tip mask="mask" mask-in="mask-in" prop-user="{{ componentsData }}"></my-tip>
<!--错误弹窗-->
<import src="../template/template"></import>
<template wx:if="{{ toast.show }}" is="showToast" data="{{ ...toast }}"></template>
<template wx:if="{{ FuckU.show != 1 }}" is="showGZH" data="{{ ...FuckU, notDel }}"></template>
