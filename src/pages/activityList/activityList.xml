<view class="p30 out-wrap">
  <navigator url="{{ options.type == '微工坊列表' ? '../coProduction/coProduction?id=' + item.id : '../activityDetail/activityDetail?id=' + item.id }}" wx:for="{{ list }}" wx:key="*55" class="ac-list br10">
    <image mode="widthFix" src="{{ imgDomain }}{{ item.picture }}"></image>
    <view class="p20">
      <view class="f32 c000 ell1">{{ item.title }}</view>
      <view class="f26 c999 ac-l-b">
        <text class="f24 cfff">{{ item.progress == 0 ? '未开始' : item.progress == 1 ? '报名中' : '已结束'}}</text>
        <text>{{ item.start_time }} ~{{ item.end_time }}</text>
      </view>
    </view>
  </navigator>
</view>
<!--错误弹窗-->
<import src="../template/template"></import>
<template wx:if="{{ toast.show }}" is="showToast" data="{{ ...toast }}"></template>
<template wx:if="{{ FuckU.show != 1 }}" is="showGZH" data="{{ ...FuckU, notDel }}"></template>
