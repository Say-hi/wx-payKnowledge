<view class="bt1 ar-info p30">
  <image class="br user-img" src="{{ testImg }}"></image>
  <view class="f24 c666">
    <view class="f32 lh40 c333 fw7">阿斯顿发</view>
    <view class="lh50">asldkfjasdf</view>
    <view class="lh50">作者: asldfja</view>
    <view class="lh50">123人关注</view>
    <view class="f32 br5 {{ follow ? 'follow' : 'no-follow' }}">
      {{ follow ? '- 取关专栏' : '+ 关注专栏' }}
    </view>
  </view>
</view>
<view class="p30 f28 tab-bg">12篇文章</view>
<view class="ar-list-w">
  <navigator url="../articleDetail/articleDetail" wx:for="12344" wx:key="*33" class="ar-list bd1 p200">
    <image mode="widthFix" class="br5 ar-l-l" src="{{ testImg }}"></image>
    <view class="ar-l-r">
      <view class="f32 c333">asdlfjasdlf;jkasdflaks</view>
      <view class="f24 c999 ar-l-r-b">
        <view>2{{ index }}天前</view>
        <view class="h-a-operation">
          <view class="jwq lh70 f24 c999 icon-pinglun">40</view>
          <view catchtap="zan" data-index="{{ index }}" data-id="{{ item.id }}" class="jwq lh70 f24 c999 {{ item.zan ? 'icon-xin1' : 'icon-xin' }}">40</view>
          <view catchtap="giveTip" data-index="{{ index }}" class="jwq lh70 f24 main-text-color icon-zhifufangshi">12</view>
        </view>
      </view>
    </view>
  </navigator>
</view>
<my-tip mask="mask" mask-in="mask-in" prop-user="{{ componentsData }}"></my-tip>
